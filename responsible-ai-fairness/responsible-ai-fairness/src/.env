PDF_CONTAINER_NAME='${pdf_containerName}'
CSV_CONTAINER_NAME='${csv_containerName}'
ZIP_CONTAINER_NAME='${zip_containerName}'

# file - storage api's
AZURE_UPLOAD_API ="${azure_upload_api}"
AZURE_GET_API = "${azure_get_api}"
GCP_UPLOAD_API = "${gcp_upload_api}"
GCP_GET_API = "${gcp_get_api}"
AWS_UPLOAD_API = "${aws_upload_api}"
AWS_GET_API = "${aws_get_api}"

# report api url

REPORT_URL = "${reporturl}"

# gemini api key
GEMINI_API_KEY="${gemini_api_key}"
GEMINI_FLASH_MODEL_NAME="${gemini_flash_model_name}"
GEMINI_PRO_MODEL_NAME="${gemini_pro_model_name}"

#openai api key
OPENAI_API_KEY="${openai_api_key}"
OPENAI_API_TYPE = "${openai_api_type}"
OPENAI_API_BASE ="${openai_api_base}"
OPENAI_API_VERSION = "${openai_api_version}"
OPENAI_ENGINE_NAME="${openai_engine_name}"

# active llm
ACTIVE_LLM = "${active_llm}" # Options: azureopenai, gemini-2.5-flash, gemini-2.5-pro, aws

# telemetry flag
tel_Falg = "${tel_Falg}"

# Mixtral Apis
MIXTRAL_URL="${mixtral_url}"
