#Database Config
DB_NAME = "${dbname}"
DB_NAME_WB = "${dbname_wb}"
DB_USERNAME = "${username}"
DB_PWD = "${password}"
DB_IP = "${ipaddress}"
DB_PORT = "${port}"
MONGO_PATH="mongodb://${DB_USERNAME}:${DB_PWD}@${DB_IP}:${DB_PORT}/"
COSMOS_PATH = "${cosmos_path}"
DB_TYPE="${dbtype}"

# blob containers
Dt_containerName = "${dt_containerName}"
Model_CONTAINER_NAME = "${model_containerName}"
